{
  "name": "javascript-starter-kit",
  "version": "1.0.0",
  "description": "JavaScript basic development environment",
  "scripts": {
    "prestart": "babel-node scripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node scripts/server.js",
    "localtunnel": "lt --port 3000 --subdomain purro",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress scripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Carlos Salas",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-preset-env": "^1.7.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chai": "4.2.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.0.0",
    "eslint": "6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-watch": "^5.1.2",
    "express": "^4.17.3",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "15.1.1",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.15.0",
    "localtunnel": "^1.9.2",
    "lodash": "^4.17.13",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "npm-run-all": "3.1.1",
    "nsp": "^3.2.1",
    "numeral": "1.5.3",
    "open": "^6.4.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "latest",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "latest",
    "webpack-md5-hash": "latest"
  }
}
